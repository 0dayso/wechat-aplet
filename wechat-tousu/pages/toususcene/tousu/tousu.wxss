page{
  background: #ffffff;
}
.jts-container{
    padding: 20px 12px;
}
.jts-complaintant{
    position: relative;
    height: 56px;
    line-height: 24px;
    font-size: 14px;
    padding-right: 24px;
}
.jts-complaintant .gray{
    color:#919191;
}
.jts-complaintant .gonextImg{
    position: absolute;
    right: 10px;
    top: 13px;
    width: 10px;
    height: 15px;
}

.jts-content input{
    outline:none;
    box-sizing:border-box;
    -webkit-box-sizing:border-box;
    width: 100%;
    height: 40px;
    border: 1px solid #D8D8D8;
    padding-left: 8px;
    border-radius: 6px;
    -webkit-border-radius: 6px;
    margin: 5px 0;
    /*background:#F7F7F7 no-repeat -4px -3px;*/
    background-color: #F7F7F7;
    font-size: 12px;
}

.cooperative-businesses{
    position: relative;
    font-size: 12px;
    height: 30px;
    padding: 12px 0;
}

.cooperative-businesses .businessesimg{
    position: relative;
    top: 0;
    width: 25px;
    height: 25px;
    border-radius: 13px;
    -webkit-border-radius: 13px;
    border: 1px solid white;
}

.images-wrapper {
  padding: 20rpx;
  background-color: #fff;
  margin-top: 20rpx;
}
.images-wrapper-text {
  font-size: 28rpx;
}
.images-text{
  font-size: 16px;
  color:#919191;
}
.images-list {
  display: flex;
  margin-top: 20rpx;
  flex-wrap: wrap;
}
.images-image {
  width: 50px;
  height: 50px;
  margin: 10rpx;
}
.images-image-plus {
  border: 1px solid #999;
}

.decodephone_box{
    margin-bottom:10px;
    color:#919191;
    margin-top:10px;
    font-size: 14px;
}

.public-btn{
    margin: 20px 0;
    background-color: #f9972b;
    color: #ffffff;
    font-size: 16px;
}
.public-button-hover {
    opacity: 0.7;
}

.jts-isagree{
    margin: 40px 0;
}