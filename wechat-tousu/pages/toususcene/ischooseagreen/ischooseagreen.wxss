.jts-content{
    padding: 16px 12px 22px 12px;
}
.jts-content .title{
    text-align: center;
    font-size: 26px;
}

.jts-content .content{
    padding-top: 23px;
    color:#515151;
}
.content-des view{
    padding-bottom: 23px;
}


.isagreebtns{
    position: relative;
    width: 100%;
    height: 65px;
}
.isagreebtns button{
    display: inline-block;
    position: absolute;
    top: 12px;
    width: 41%;
    height: 44px;
}
.isagreebtns .left-btn{
    left: 6%;
    background-color: #f9972b;
    color: #ffffff;
}
.isagreebtns .right-btn{
    right: 6%;
}
.left-button-hover {
  background-color: #f9972b;
  opacity: 0.7;
}

.next-btn{
    background-color: #f9972b;
    color: #ffffff;
    font-size: 16px;
}
.next-button-hover {
    opacity: 0.7;
}